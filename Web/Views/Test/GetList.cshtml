@using Entities.InterjectionDTO
@model List<DisciplineDTO>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table table-bordered">
    <tr>
        <th>Type</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.DisciplineType</td>
        </tr>
    }
</table>

<br />



